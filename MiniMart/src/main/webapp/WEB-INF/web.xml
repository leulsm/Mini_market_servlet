<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>MiniMart</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<servlet-class>Register</servlet-class>
  </servlet>   
   <servlet>
  	<servlet-name>adminpageserv</servlet-name>
  	<servlet-class>adminpageserv</servlet-class>
  </servlet>  
  <servlet>
  	<servlet-name>DisplayAll</servlet-name>
  	<servlet-class>DisplayAll</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addItem</servlet-name>
  	<servlet-class>addItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>Logout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>manageitemserv</servlet-name>
  	<servlet-class>manageitemserv</servlet-class>
  </servlet>
  
  
  
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayAll</servlet-name>
  	<url-pattern>/DisplayAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addItem</servlet-name>
  	<url-pattern>/addItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>adminpageserv</servlet-name>
  	<url-pattern>/adminpageserv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>manageitemserv</servlet-name>
  	<url-pattern>/manageitemserv</url-pattern>
  </servlet-mapping>
</web-app>